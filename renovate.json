{
  "extends": [
    "config:base",
    "docker:enableMajor",
    ":disableRateLimiting",
    ":automergePr",
    ":automergeRequireAllStatusChecks",
    ":semanticCommits",
    ":maintainLockFilesWeekly"
  ],
  "kubernetes": {
    "fileMatch": [".+\\.yaml(.jinja2)?$"]
  },
  "labels": ["renovate"],
  "major": {
    "labels": ["major", "renovate"]
  },
  "minor": {
    "labels": ["minor", "renovate"]
  },
  "patch": {
    "labels": ["patch", "renovate"]
  },
  "pin": {
    "labels": ["pin", "renovate"]
  },
  "pinDigests": true
}
